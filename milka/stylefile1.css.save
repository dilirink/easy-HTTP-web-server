body{
    margin: 0;
    background-image:url(first_bg-min.jpg);
    background-size: cover;
}

 #card_wifi ul{
	display: flex;
	justify-content:space-between; 
	padding-left: 0; 
	padding-bottom: 75px;
} 
#card_wifi li{
    display: flex;
	justify-content:space-between; 
	margin: 0px;
	padding: 0px;
}
.Ml{
    text-align: center;
}
.blockConst{
    box-shadow: 0px 4px 3px 0px rgba(34, 34, 34, 0.575);
    padding: 3% 3% 1% 3%;
    background-color: #f2f2f2;
    border-radius: 40px;
    
}
.blockConst h1{
   /*  font-family: cursive; */
    font-size: 24px;
    text-align: center;
    padding-top: 10px;
    margin-top: 0;
}
.blockConst h2{
    /* font-family: cursive; */
    font-size: 15px;
    text-align: center;
    padding: 0 15px;
}
.blockConst p{
    background-color: #dadada;
    text-decoration: underline;
    text-align: justify;
    padding: 5%;
    border-radius: 20px;
}
.photo{
    margin-top: 20px;
    text-align: center;
}
.photo a{
    background-image: linear-gradient(180deg, rgb(245, 245, 245) 0%, rgb(228, 228, 228) 50%, rgb(184, 184, 184) 100%);
    border: 2px solid #fff;
    text-decoration: none;
    border-radius: 20px;
    padding: 9px 25px;
    font-size: 18px;
}
.razrb{
    margin-top: 25px;
    text-align: center;
}
/* ------------------CONECT------------------ */
.conectright {
    margin-left: 1px;
    background-image: linear-gradient(180deg, rgb(245, 245, 245) 0%, rgb(228, 228, 228) 50%, rgb(184, 184, 184) 100%);
    display: block;
    margin-top: 100%;
    height: 142px;
    padding: 4px;
    border-radius: 0px 10px 10px 0px;
    font-weight: lighter;
    font-size: 16px;
    box-shadow: 10px 0 20px -20px #000 inset;
} 
.VK{
    margin-top: 5px;
}
.Mail{
    margin-top: 5px;
}
.conectleft {
    margin-right: 1px;
    background-image: linear-gradient(180deg, rgb(245, 245, 245) 0%, rgb(228, 228, 228) 50%, rgb(184, 184, 184) 100%);
    display: block;
    margin-top: 100%;
    height: 142px;
    padding: 4px;
    border-radius: 10px 0 0 10px;
    font-weight: lighter;
    font-size: 16px;
    box-shadow: -12px 0 20px -20px #000 inset;
} 
.conect_modal1{
    text-decoration-line: underline;
    color: rgb(15, 15, 15);
    /* font-family: cursive; */
    font-weight: lighter;
    font-size: 12px;
}
.conect_modal1:hover{
    text-decoration: none;
    color:#000;
}
/* ------------------Модальные окна------------------ */
.modal {
    position: fixed;
    display: none;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 0;
    background: rgba(248, 248, 248, 0.8);
}
.modal:target {
    display: flex;
} 
